import com.jxl.shuriken.core.UIComponent;import com.jxl.shuriken.controls.LinkButton;import com.jxl.shuriken.events.ShurikenEvent;import com.jxl.shuriken.events.Callback;import com.jxl.shuriken.events.Event;class com.jxl.threesixtyflex.views.SettingsScreen extends UIComponent{	public static var SYMBOL_NAME:String 		= "com.jxl.threesixtyflex.views.SettingsScreen";		public static var EVENT_UPDATE:String		= "update";	public static var EVENT_BACK:String 		= "back";	public static var EVENT_DELETE:String		= "delete";		private var __sTitle_mc:MovieClip;	private var __update_lb:LinkButton;	private var __back_lb:LinkButton;	private var __delete_lb:LinkButton;	private var __copy_txt:TextField;		private var __updateCallback:Callback;	private var __backCallback:Callback;	private var __deleteCallback:Callback;		public function SettingsScreen()	{	}		public function onLoad():Void	{		Selection.setFocus(__update_lb);	}		private function createChildren():Void	{		super.createChildren();				if(__sTitle_mc == null)		{			__sTitle_mc = attachMovie("SettingsTitle", "__sTitle_mc", getNextHighestDepth());		}				if(__update_lb == null)		{			__update_lb = LinkButton(createComponent(LinkButton, "__update_lb"));			__update_lb.setReleaseCallback(this, onUp);			var uTF:TextFormat = __update_lb.textField.getTextFormat();			uTF.bold = true;			__update_lb.textField.setNewTextFormat(uTF);			__update_lb.label = "Check For Updates";		}				if(__back_lb == null)		{			__back_lb = LinkButton(createComponent(LinkButton, "__back_lb"));			__back_lb.setReleaseCallback(this, onBack);			var bTF:TextFormat = __back_lb.textField.getTextFormat();			bTF.bold = true;			__back_lb.textField.setNewTextFormat(bTF);			__back_lb.label = "Back to Main.";		}				if(__delete_lb == null)		{			__delete_lb = LinkButton(createComponent(LinkButton, "__delete_lb"));			__delete_lb.setReleaseCallback(this, onDelete);			__delete_lb.label = "Delete Event Cache";		}				if(__copy_txt == null)		{			__copy_txt = createLabel("__copy_txt");			__copy_txt.embedFonts = true;			__copy_txt.setNewTextFormat(_global.pixelTF);			__copy_txt.text = "created by jesse warden";		}	}		private function redraw():Void	{		super.redraw();				__sTitle_mc._x = 22;		__sTitle_mc._y = 22;				__update_lb.move(__sTitle_mc._x + 4, __sTitle_mc._y  + __sTitle_mc._height + 22);		__update_lb.setSize(__update_lb.textField.textWidth + 4, __update_lb._height);				__delete_lb.setSize(__delete_lb.textField.textWidth + 4, __delete_lb.height);		__delete_lb.move(__update_lb._x, __update_lb._y + __update_lb._height + 4);				__back_lb.move(__delete_lb.x, __delete_lb.y + __delete_lb.height + 22);		__back_lb.setSize(__back_lb.textField.textWidth + 4, __back_lb._height);				__copy_txt.setSize(__copy_txt.textWidth + 4, __copy_txt.textHeight + 4);		__copy_txt.move(__back_lb._x, __back_lb.y + __back_lb.height + 22);	}		private function onUp(event:ShurikenEvent):Void	{		__updateCallback.dispatch(new Event(EVENT_UPDATE, this));	}		private function onBack(event:ShurikenEvent):Void	{		__backCallback.dispatch(new Event(EVENT_BACK, this));	}		private function onDelete(event:ShurikenEvent):Void	{		__deleteCallback.dispatch(new Event(EVENT_DELETE, this));	}		public function setUpdateCallback(scope:Object, func:Function):Void	{		__updateCallback = new Callback(scope, func);	}		public function setBackCallback(scope:Object, func:Function):Void	{		__backCallback = new Callback(scope, func);	}		public function setDeleteCallback(scope:Object, func:Function):Void	{		__deleteCallback = new Callback(scope, func);	}}